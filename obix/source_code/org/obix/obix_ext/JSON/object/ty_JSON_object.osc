type JSON_object	inherit JSON_value end		inherit !collection<JSON_object_member> end		command contains_value		in member_id type:simple_string end				out result type:yes_no end	end		command require_value		in member_id type:simple_string check: i_object_.contains_value ( i_member_id ) end				out result type:JSON_value end	end		command get_value		in member_id type:simple_string end				out result type:JSON_value voidable:yes end	end		// get/require typed values		// object	command require_object_value		in member_id type:simple_string check: i_object_.contains_value ( i_member_id ) end		%system_error_handler_input_argument				%result_or_JSON_parsing_error_output < JSON_object >	end		command get_object_value		in member_id type:simple_string end		%system_error_handler_input_argument				%result_or_JSON_parsing_error_output < JSON_object >	end	// TD array		// string		command require_string_value		in member_id type:simple_string check: i_object_.contains_value ( i_member_id ) end		%system_error_handler_input_argument				%result_or_JSON_parsing_error_output < string >	end		command get_string_value		in member_id type:simple_string end		%system_error_handler_input_argument				%result_or_JSON_parsing_error_output < string >	end	// integers		command require_integer_value		in member_id type:simple_string check: i_object_.contains_value ( i_member_id ) end		%system_error_handler_input_argument				%result_or_JSON_parsing_error_output < signed_integer32 >	end		command get_integer_value		in member_id type:simple_string end		%system_error_handler_input_argument				%result_or_JSON_parsing_error_output < signed_integer32 >	end		command require_positive32_value		in member_id type:simple_string check: i_object_.contains_value ( i_member_id ) end		%system_error_handler_input_argument				%result_or_JSON_parsing_error_output < positive32 >	end		command get_positive32_value		in member_id type:simple_string end		%system_error_handler_input_argument				%result_or_JSON_parsing_error_output < positive32 >	end		// yes_no		command require_yes_no_value		in member_id type:simple_string check: i_object_.contains_value ( i_member_id ) end		%system_error_handler_input_argument				%result_or_JSON_parsing_error_output < yes_no >	end		command get_yes_no_value		in member_id type:simple_string end		%system_error_handler_input_argument				%result_or_JSON_parsing_error_output < yes_no >	endend