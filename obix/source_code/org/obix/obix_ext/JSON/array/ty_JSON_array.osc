type JSON_array	inherit JSON_value end	inherit !collection<JSON_value> end		/// TD	command first_value		in_check check: not i_object_.is_empty end		out result type:JSON_value end	end	command last_value		in_check check: not i_object_.is_empty end		out result type:JSON_value end	end	end ///		command value_at_index		in index type:positive32 check: i_index <= i_object_.item_count end		out result type:JSON_value end	endend